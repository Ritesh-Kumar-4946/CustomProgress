<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="ua_UnacademyLoader">
        <attr name="ua_circle_style">
            <enum name="butt" value="0" />
            <enum name="round" value="1" />
            <enum name="square" value="2" />
        </attr>
        <attr name="ua_progress" format="integer" />
        <attr name="ua_max" format="integer" />
        <attr name="ua_negative_enabled" format="boolean" />
        <attr name="ua_move_outside_circle" format="boolean" />
        <attr name="ua_maintain_equal_circle" format="boolean" />
        <attr name="ua_use_custom_radii" format="boolean" />
        <attr name="ua_lock_enabled" format="boolean" />
        <attr name="ua_circle_x_radius" format="dimension" />
        <attr name="ua_circle_y_radius" format="dimension" />
        <attr name="ua_circle_stroke_width" format="dimension" />
        <attr name="ua_disable_pointer" format="boolean" />
        <attr name="ua_pointer_stroke_width" format="dimension" />
        <attr name="ua_pointer_halo_width" format="dimension" />
        <attr name="ua_pointer_halo_border_width" format="dimension" />
        <attr name="ua_circle_fill" format="color" />
        <attr name="ua_circle_color" format="color" />
        <attr name="ua_circle_progress_color" format="color" />
        <attr name="ua_pointer_color" format="color" />
        <attr name="ua_pointer_halo_color" format="color" />
        <attr name="ua_pointer_halo_color_ontouch" format="color" />
        <attr name="ua_pointer_alpha_ontouch" format="integer" />
        <attr name="ua_pointer_angle" format="float" />
        <attr name="ua_start_angle" format="float" />
        <attr name="ua_end_angle" format="float" />
        <attr name="ua_disable_progress_glow" format="boolean" />
        <attr name="ua_hide_progress_when_empty" format="boolean" />
    </declare-styleable>

</resources>
